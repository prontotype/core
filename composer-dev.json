    "require": {
        "twig/twig": "~1.16",
        "symfony/console": "2.6.*",
        "dflydev/apache-mime-types": "~1.0",
        "rdlowrey/auryn": "~0.14",
        "illuminate/support": "~4.2",
        "league/event": "~2.0",
        "allmarkedup/twig-extensions": "~0.2",
        "allmarkedup/super-sharp-router": "~0.2",
        "allmarkedup/conph": "~0.1",
        "allmarkedup/ffs": "~0.1",
        "prontotype/composer-installer": "~0.1",
        "prontotype/data-plugin": "dev-master",
        "prontotype/user-routes-plugin": "dev-master",
        "prontotype/short-urls-plugin": "dev-master"
